<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="325" d:DesignHeight="48"
             x:Class="PamukkyDesktopClient.Views.guserli">
	<DockPanel>
		<Border Name="pfpborder" CornerRadius="14" ClipToBounds="True" Height="28" Background="White" Margin="6" VerticalAlignment="Center">
			<Image Name="pfp" Width="28" Height="28" Source="/Assets/person.png"></Image>
		</Border>
		<StackPanel VerticalAlignment="Center" DockPanel.Dock="Right" Orientation="Horizontal" Name="rc">
			<Label VerticalAlignment="Center">Role: </Label>
		</StackPanel>
		<Label Name="uname" VerticalAlignment="Center">User</Label>
	</DockPanel>
</UserControl>
