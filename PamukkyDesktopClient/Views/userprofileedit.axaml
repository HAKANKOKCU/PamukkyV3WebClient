<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="450" d:DesignHeight="380"
             x:Class="PamukkyDesktopClient.Views.userprofileedit">
	<StackPanel>
		<Border HorizontalAlignment="Center" CornerRadius="80" ClipToBounds="True" Margin="8" Background="Gray">
			<Image Name="pfp" Width="160" Height="160"></Image>
		</Border>
		<DockPanel>
			<Label Padding="6" VerticalAlignment="Center">Name</Label>
			<TextBox IsReadOnly="True" Margin="6" Watermark="Name is required" Name="nametb"></TextBox>
		</DockPanel>
		<DockPanel>
			<Label Padding="6" VerticalAlignment="Center">Bio</Label>
			<TextBox IsReadOnly="True" Margin="6" Watermark="Enter bio here" AcceptsReturn="True" TextWrapping="Wrap" Name="biotb"></TextBox>
		</DockPanel>
		<Button Margin="6 6 6 0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Background="Transparent" Name="cpassbtn">Change Password</Button>
		<Button Margin="6 0 6 6" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Background="Transparent" Name="logotbtn">Logout</Button>
		<Label Name="elbl"></Label>
	</StackPanel>
</UserControl>
